{
  "layout": "base",
  "title": "3D Cube",
  "scripts": ["/js/lib/modernizr.js","/js/audio.js"],
  "css": ["/css/audio.css"]
}
______

<div class="hero">
  <h2>HTML5 Audio Player</h2>
  <h3 class="subtitle"></h3>

</div>

<div id="player">
  <div id="playpause"> </div>
  <div id="vol"><input type="range" value="10" id="volume" max="11" min="1" step="0.01"></div>
  <div id="stop" title="Stop"> </div>
  <div id="nowplaying">
    <p class="title"> </p>
    <p class="artist"> </p>
    <p class="album"> </p>
    <img class="art" />
    <p class="year"> </p>
  </div>

  <input type="range" value="0" id="seek" />

  <div id="autoplay" class="active">Autoplay</div>

</div>
<ul id="playlist">
  <li class="songs"><input style="opacity:0" type="file" id="choosefiles" multiple /></li>
</ul>

